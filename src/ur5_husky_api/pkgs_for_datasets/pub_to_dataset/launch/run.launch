<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="robot_ip" default="127.0.0.1"/>

    <!-- Эта нода публикует информацию о состоянии робота-манипулятора -->
    <node name="arm_msgs" pkg="arm_msgs" type="arm_msgs" output="screen">
        <param name="robot_ip" type="string" value="$(arg robot_ip)"/>
    </node>

</launch>

